model Post {
    fields {
        views Number
    }

    actions {
        update incrementViews(id) {
            //expect-error:29:31:AttributeExpressionError:Operator '+=' not permitted on @set
            @set(post.views += 1)
        }
        update decrementViews(id) {
            //expect-error:29:31:AttributeExpressionError:Operator '-=' not permitted on @set
            @set(post.views -= 1)
        }
        update compareViews(id) {
            //expect-error:29:31:AttributeExpressionError:Operator '==' not permitted on @set
            @set(post.views == 1)
        }
    }
}
