model Post {
    fields {
        status PostStatus
    }

    actions {
        update published(id) {
            //expect-error:18:27:AttributeExpressionError:expression expected to resolve to type PostStatus but it is OtherPostStatus
            @set(post.status = OtherPostStatus.Something)
        }
    }
}

enum PostStatus {
    Published
    Draft
}

enum OtherPostStatus {
    Something
    Else
}
