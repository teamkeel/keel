{{- range .Models }}
export interface {{ .Name }} {
  {{- range .Fields }}
  {{ .Name }}{{ if .Optional }}?{{end}}: {{ .Type }}
  {{- end }}
}
export type {{ .Name }}Query = Partial<{
  {{- range .Fields }}
  {{ .Name }}?: QueryConstraints.{{ .ConstraintType }}
  {{- end }}
}>

export type {{ .Name }}UniqueFields = Partial<{
  {{- range .UniqueFields }}
  {{ .Name }}?: QueryConstraints.{{ .ConstraintType }}
  {{- end }}
}>

{{- end }}
{{- range .Enums }}
export enum {{ .Name }} {
  {{- range .Values }}
  {{ .Label }} = "{{ .Label }}",
  {{- end }}
}
{{- end }}
