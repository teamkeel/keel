{
  "name": "@teamkeel/functions-runtime",
  "version": "0.0.1",
  "description": "Internal package used by @teamkeel/sdk",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "type": "module",
  "scripts": {
    "test": "vitest run --reporter verbose --pool=threads --poolOptions.threads.singleThread",
    "format": "npx prettier --write src/**/*.js",
    "build": "tsup",
    "type-check": "tsc --noEmit",
    "dev": "tsup --watch",
    "generate-openapi": "tsx scripts/oas.ts",
    "postinstall": "npm run generate-openapi"
  },
  "keywords": [],
  "author": "Keel (www.keel.so)",
  "license": "ASL (Apache 2.0)",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "scripts",
    "README.md"
  ],
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@types/pg": "^8.11.14",
    "@types/ws": "^8.18.1",
    "prettier": "3.1.1",
    "tsup": "^8.0.0",
    "tsx": "^4.19.4",
    "typescript": "^5.3.0",
    "typescript-json-schema": "^0.65.1",
    "vitest": "3.0.8"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "~3.722.0",
    "@aws-sdk/credential-providers": "~3.721.0",
    "@aws-sdk/s3-request-presigner": "~3.722.0",
    "@neondatabase/serverless": "^1.0.0",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/exporter-trace-otlp-proto": "^0.57.2",
    "@opentelemetry/resources": "^1.30.1",
    "@opentelemetry/sdk-trace-base": "^1.30.1",
    "@opentelemetry/sdk-trace-node": "^1.30.1",
    "alterschema": "^1.1.3",
    "change-case": "^4.1.2",
    "json-rpc-2.0": "^1.7.0",
    "ksuid": "^3.0.0",
    "kysely": "^0.27.6",
    "pg": "^8.14.1",
    "postgres-interval": "^4.0.2",
    "traceparent": "^1.0.0",
    "vite": "^6.2.5",
    "ws": "^8.18.1"
  },
  "packageManager": "pnpm@9.12.3+sha512.cce0f9de9c5a7c95bef944169cc5dfe8741abfb145078c0d508b868056848a87c81e626246cb60967cbd7fd29a6c062ef73ff840d96b3c86c40ac92cf4a813ee"
}
