model Thing {
    fields {
        texts Text[]?
        numbers Number[]?
        dates Date[]?
        booleans Boolean[]?
        timestamps Timestamp[]?
        enums MyEnum[]?
        files File[]?
        durations Duration[]?
    }
    actions {
        create createThing() with (texts?, numbers?, dates?, booleans?, timestamps?, enums?, files?, durations?) {
            @function
            @permission(expression: true)
        }
    }
}

enum MyEnum {
    One 
    Two
    Three 
}