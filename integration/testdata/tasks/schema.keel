task EmptyFlow {
    fields {
        orderDate Date
        shipByDate Date
    }

    @permission(roles: [User])
    @orderBy(shipByDate: asc, orderDate: asc)
}

enum Priority {
    Low
    Medium
    High
}

task InputsTask {
    fields {
        textField Text
        numberField Number
        booleanField Boolean
        dateField Date
        timestampField Timestamp
        decimalField Decimal
        enumField Priority
        optionalTextField Text?
    }

    @permission(roles: [User])
}

role User {
    domains {
        "keel.xyz"
    }
}
