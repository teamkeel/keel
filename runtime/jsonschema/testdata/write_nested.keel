message PersonResult {
    name Text
}

message PersonPayload {
    name Text
    nested NestedObject
    optionalNested NestedObject?
}

message NestedObject {
    bool Boolean
}

model Person {
    fields {
        name Text
    }

    functions {
        write testAction(PersonPayload) returns (PersonResult)
    }
}

api Test {
    models {
        Person
    }
}
