message Meta {
  creatorUsername Text
}

message UploadPayload {
  people Person[]
  meta Meta
}

message UploadOutput {
  createdCount Number
  meta Meta
}

message SearchPayload {
  funnyName Text
}

message SearchOutput {
  people Person[]
}

model Person {
  fields {
    name Text
  }

  functions {
    write upload(UploadPayload) returns(UploadOutput)
    read search(SearchPayload) returns(SearchOutput)
  }
}

api Web {
  models {
    Person
  }
}