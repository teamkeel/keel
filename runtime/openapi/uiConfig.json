      "required": [
        "__type",
        "autoContinue",
        "duplicateHandling",
        "mode",
        "name"
      ]
    },
    "ScanDuplicateMode": {
      "description": "Defines how duplicate scans should be handled.",
      "default": "none",
      "enum": [
        "none",
        "rejectDuplicates",
        "trackQuantity"
      ],
      "type": "string"
    },
    "UiElementInputDatePickerApiResponse": {
      "type": "object",
      "properties": {
        "mode": {
          "description": "Defines what type of datepicker should we use.",
          "default": "dateTime",
          "enum": [
            "date",
            "dateTime"
          ],
          "type": "string"
        },
        "min": {
          "type": "string"
        },
        "max": {
          "type": "string"
        },
        "__type": {
          "type": "string",
          "const": "ui.input.datePicker"
        },
        "name": {
          "type": "string"
        },
        "label": {
          "type": "string"
        },
        "defaultValue": {
          "type": "string"
        },
        "optional": {
          "type": "boolean"
        },
        "disabled": {
          "type": "boolean"
        },
        "helpText": {
          "type": "string"
        },
        "validationError": {
          "type": "string"
        }
