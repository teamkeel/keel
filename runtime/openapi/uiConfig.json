    "UiElementDividerApiResponse": {
      "type": "object",
      "properties": {
        "__type": {
          "type": "string",
          "const": "ui.display.divider"
        }
      },
      "additionalProperties": false,
      "required": [
        "__type"
      ]
    },
    "UiElementMarkdownApiResponse": {
      "type": "object",
      "properties": {
