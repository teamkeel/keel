            "additionalProperties": false,
            "required": [
              "index",
              "name"
            ]
          }
        },
        "__type": {
          "type": "string",
          "const": "ui.display.table"
        }
      },
      "additionalProperties": false,
      "required": [
        "__type",
        "columns",
        "data"
      ]
    },
    "UiElementKeyValueApiResponse": {
      "type": "object",
      "properties": {
        "data": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "key": {
                "type": "string"
              },
              "value": {
                "anyOf": [
                  {
                    "type": "string",
                    "format": "date-time"
                  },
                  {
                    "type": [
                      "string",
                      "number",
                      "boolean"
                    ]
                  }
                ]
              }
            },
            "additionalProperties": false,
            "required": [
              "key",
              "value"
            ]
          }
        },
        "mode": {
          "$ref": "#/$defs/KeyValueMode"
        },
        "__type": {
          "type": "string",
          "const": "ui.display.keyValue"
        }
      },
      "additionalProperties": false,
      "required": [
        "__type",
        "data",
        "mode"
      ]
    },
    "KeyValueMode": {
      "enum": [
        "card",
        "grid",
        "list"
      ],
      "type": "string"
    },
    "UiElementFileApiResponse": {
      "type": "object",
      "properties": {
        "title": {
          "type": "string"
        },
        "file": {
          "additionalProperties": false,
          "type": "object",
          "properties": {
            "key": {
              "type": "string"
            },
            "filename": {
              "type": "string"
            },
            "contentType": {
              "type": "string"
            },
            "size": {
              "type": "number"
            },
            "url": {
              "type": "string"
            }
          },
          "required": [
            "contentType",
            "filename",
            "key",
            "size",
            "url"
          ]
        },
        "__type": {
          "type": "string",
          "const": "ui.display.file"
        }
