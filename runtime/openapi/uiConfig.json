            }
          ]
        },
        "optional": {
          "type": "boolean"
        },
        "disabled": {
          "type": "boolean"
        },
        "helpText": {
          "type": "string"
        },
        "validationError": {
          "type": "string"
        }
      },
      "additionalProperties": false,
      "required": [
        "__type",
        "disabled",
        "label",
        "name",
        "optional",
        "options"
      ]
    },
    "ElementDataType": {
      "anyOf": [
        {
          "type": "string",
          "format": "date-time"
        },
        {
          "type": [
            "string",
            "number",
            "boolean"
          ]
        }
      ]
    },
    "UiElementSelectTableApiResponse": {
      "type": "object",
      "properties": {
        "data": {
          "type": "array",
          "items": {
            "$ref": "#/$defs/Record<string,any>"
          }
        },
        "columns": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "name": {
