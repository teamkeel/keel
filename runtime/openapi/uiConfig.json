    "UiElementSelectOneApiResponse": {
      "type": "object",
      "properties": {
        "options": {
          "type": "array",
          "items": {
            "anyOf": [
              {
                "type": "string",
                "format": "date-time"
              },
              {
                "type": "object",
                "properties": {
                  "label": {
                    "type": "string"
                  },
                  "value": {
                    "$ref": "#/$defs/ElementDataType"
                  }
                },
                "additionalProperties": false,
                "required": [
                  "label",
                  "value"
                ]
              },
              {
                "type": [
                  "string",
                  "number",
                  "boolean"
                ]
              }
            ]
          }
        },
        "__type": {
          "type": "string",
          "const": "ui.select.one"
        },
        "name": {
          "type": "string"
        },
        "label": {
          "type": "string"
        },
        "defaultValue": {
          "anyOf": [
            {
              "type": "string",
              "format": "date-time"
            },
            {
              "type": [
                "string",
                "number",
                "boolean"
