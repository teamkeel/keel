      "enum": [
        "boolean",
        "hidden",
        "id",
        "number",
        "text"
      ],
      "type": "string"
    },
    "UiElementInputScanApiResponse": {
      "type": "object",
      "properties": {
        "mode": {
          "enum": [
            "multi",
            "single"
          ],
          "type": "string"
        },
        "title": {
          "type": "string"
        },
        "description": {
          "type": "string"
        },
        "unit": {
          "type": "string"
        },
        "max": {
          "type": "number"
        },
        "min": {
          "type": "number"
        },
        "duplicateHandling": {
          "$ref": "#/$defs/ScanDuplicateMode"
        },
        "autoContinue": {
          "type": "boolean"
        },
        "__type": {
          "type": "string",
          "const": "ui.input.scan"
        },
        "name": {
          "type": "string"
        },
        "validationError": {
          "type": "string"
