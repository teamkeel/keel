            "additionalProperties": false,
            "required": [
              "index",
              "name",
              "validationError"
            ]
          }
        },
        "__type": {
          "type": "string",
          "const": "ui.iterator"
        }
      },
      "additionalProperties": false,
      "required": [
        "__type",
        "content",
        "name"
      ]
    },
    "UiElementApiResponses": {
      "type": "array",
      "items": {
        "$ref": "#/$defs/UiElementApiResponse"
      }
    },
    "UiElementPrintApiResponse": {
      "type": "object",
      "properties": {
        "title": {
          "type": "string"
