{
  "anyOf": [
    {
      "$ref": "#/$defs/UiCompleteApiResponse"
    },
    {
      "$ref": "#/$defs/UiPageApiResponse"
    }
  ],
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$defs": {
    "UiCompleteApiResponse": {
      "type": "object",
      "properties": {
        "stage": {
          "type": "string"
        },
        "title": {
          "type": "string"
        },
        "description": {
          "type": "string"
        },
        "content": {
          "$ref": "#/$defs/UiElementApiResponses"
        },
        "autoClose": {
          "type": "boolean"
        },
        "fullWidth": {
          "type": "boolean"
        },
        "allowRestart": {
          "additionalProperties": false,
          "type": "object",
          "properties": {
            "inputs": true,
            "mode": {
              "description": "If auto, the flow will restart automatically with a notification instead of a completion screen.\nIf manual, a button will be shown on the completion screen.\nDefault is manual.",
              "enum": [
                "auto",
                "manual"
              ],
              "type": "string"
            },
            "buttonLabel": {
              "type": "string"
            }
          }
        },
        "__type": {
          "type": "string",
          "const": "ui.complete"
        }
      },
      "additionalProperties": false,
      "required": [
        "__type",
        "content"
      ]
    },
    "UiElementApiResponse": {
      "anyOf": [
        {
          "$ref": "#/$defs/UiElementDividerApiResponse"
