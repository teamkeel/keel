    "Record<string,any>": {
      "type": "object",
      "additionalProperties": false
    },
    "SelectMode": {
      "enum": [
        "multi",
        "single"
      ],
      "type": "string"
    },
    "UiElementIteratorApiResponse": {
      "type": "object",
      "properties": {
        "name": {
          "type": "string"
        },
        "content": {
          "$ref": "#/$defs/UiElementApiResponses"
        },
        "max": {
          "type": "number"
        },
        "min": {
          "type": "number"
        },
        "validationError": {
          "type": "string"
        },
