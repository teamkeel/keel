        "optional": {
          "type": "boolean"
        },
        "disabled": {
          "type": "boolean"
        },
        "helpText": {
          "type": "string"
        },
        "validationError": {
          "type": "string"
        }
      },
      "additionalProperties": false,
      "required": [
        "__type",
        "disabled",
        "label",
        "mode",
        "name",
        "optional"
      ]
    },
    "UiElementInputDataGridApiResponse": {
      "type": "object",
      "properties": {
        "data": {
          "type": "array",
          "items": true
        },
        "columns": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "key": {
                "type": "string"
              },
              "label": {
                "type": "string"
              },
              "index": {
                "type": "number"
              },
              "type": {
                "$ref": "#/$defs/DataGridDataTypes"
              },
              "editable": {
                "type": "boolean"
