        "optional": {
          "type": "boolean"
        },
        "disabled": {
          "type": "boolean"
        },
        "helpText": {
          "type": "string"
        },
        "validationError": {
          "type": "string"
        }
      },
      "additionalProperties": false,
      "required": [
        "__type",
        "disabled",
        "label",
        "name",
        "optional"
      ]
    },
