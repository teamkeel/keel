        },
        "__type": {
          "type": "string",
          "const": "ui.display.banner"
        }
      },
      "additionalProperties": false,
      "required": [
        "__type",
        "description",
        "mode",
        "title"
      ]
    },
    "BannerMode": {
      "enum": [
        "error",
        "info",
        "success",
        "warning"
      ],
      "type": "string"
    },
    "UiElementImageApiResponse": {
      "type": "object",
      "properties": {
        "url": {
          "type": "string"
        },
        "alt": {
          "type": "string"
        },
        "size": {
          "enum": [
            "full",
            "large",
            "medium",
            "small",
            "thumbnail"
          ],
          "type": "string"
        },
        "caption": {
          "type": "string"
        },
        "__type": {
          "type": "string",
          "const": "ui.display.image"
        }
      },
      "additionalProperties": false,
      "required": [
        "__type",
        "url"
      ]
    },
    "UiElementCodeApiResponse": {
      "type": "object",
      "properties": {
        "code": {
          "type": "string"
