        {
          "$ref": "#/$defs/UiElementIteratorApiResponse"
        },
        {
          "$ref": "#/$defs/UiElementPrintApiResponse"
        },
        {
          "$ref": "#/$defs/UiElementPickListApiResponse"
        }
