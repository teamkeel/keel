message PersonResult {
    text Text
    optionalText Text?
}

message PersonPayload {
    name Text
    optionalName Text?
}

model Person {
    fields {
        name Text
    }

    functions {
        read readPerson(name) returns (PersonResult)
        read readPersonWithMessage(PersonPayload) returns (PersonResult)
    }
}

api Test {
    models {
        Person
    }
}
