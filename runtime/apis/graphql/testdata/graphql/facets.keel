model Order {
    fields {
        quantity Number
        price Decimal
        category Text
        status Status
        orderDate Timestamp?
    }
    actions {
        list listOrders(category?, price?) {
            @facet(quantity, price, status, category, orderDate)
            @where(order.status != Status.Cancelled)
        }
        create createOrder() with (quantity, price, category, status, orderDate) {
            @permission(expression: true)
        }
    }
}

enum Status {
    Complete
    InProgress
    Cancelled
}

api Test {
    models {
        Order
    }
}
