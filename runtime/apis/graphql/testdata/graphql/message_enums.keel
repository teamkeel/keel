message Payload {
    sports Sport[]
    favouriteSport Sport?
}

message Result {
    sports Sport[]
    favouriteSport Sport?
}

model Person {
    fields {
        name Text
    }

    functions {
        write writeSportInterests(Payload) returns (Result)
    }
}

enum Sport {
    Cricket
    Rugby
    Soccer
}

api Test {
    models {
        Person
    }
}
