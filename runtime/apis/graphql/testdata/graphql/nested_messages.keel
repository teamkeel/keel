message PersonResult {
    nested NestedResponseObject
}

message ReadPersonPayload {
    nested NestedInputObject
}

message WritePersonPayload {
    nested NestedInputObject
}

message NestedResponseObject {
    bool Boolean
}

message NestedInputObject {
    bool Boolean
}

model Person {
    fields {
        name Text
    }

    functions {
        read readPerson(ReadPersonPayload) returns (PersonResult)
        write writePerson(WritePersonPayload) returns (PersonResult)
    }
}

api Test {
    models {
        Person
    }
}
